{
  "name": "aws-converter-api",
  "version": "1.0.0",
  "description": "Serverless converter-api",
  "main": "src/handler.js",
  "scripts": {
    "clean": "rm -rf .serverless || true && rm -rf coverage || true",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "prepackage": "npm test",
    "package": "sls package",
    "predeploy": "npm test",
    "deploy": "sls deploy",
    "undeploy": "sls remove"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "properties-to-json": "^0.1.4",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "^3.4.5",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "author": "Mario Tema <mario.tema@gmx.de>",
  "license": "MIT"
}
